<Window x:Class="Anime47VideoDownloader.FansubChooser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anime47VideoDownloader"
        mc:Ignorable="d"
        Title="FansubChooser" Height="250" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="68"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Text="Choose a fansub u want to download: If u want more about fansub visit anime47.com" FontSize="24" TextWrapping="WrapWithOverflow"/>
        <Grid x:Name="fansubContainer" Grid.Row="1"/>
    </Grid>
</Window>
